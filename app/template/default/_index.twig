            function(start, end, label) {
              // alert("Your chosen date: "+start.format('MM/DD/YYYY')+' to '+end.format('MM/DD/YYYY'));
              var index = $('.active').find('a').attr('href').substring(4);
      
              var str_select = '#country_select_' + index;
              var str_price = '#price_' + index;
              // var jsonData = JSON.parse(arr[0]);
              // var $select = $(str_select);
              
              var $select = $(str_select);
              var $price = $(str_price);
              // var country = $select[0].value;
