    $('.country').change(function() {    
        var index = $('.active').find('a').attr('href').substring(4);
        var str_daterange = '#daterange_' + index;
        var $daterange = $(str_daterange);
        var range = $daterange[0].value;
        var dates = [];

        if (range.includes('～')) {
          var dates = range.split('～');
        } else if (range.includes('-')) {
          var dates = range.split('-');
        }

        if (dates.length > 0) {
          var str_select = '#country_select_' + index;
          var $select = $(str_select);
          var country = $select[0].value;
          var start = new Date(dates[0].trim());
          var end = new Date(dates[1].trim());
          end.setFullYear(start.getFullYear());
          calculatePrice(country, changeDateFormat(start, '-'), changeDateFormat(end, '-'));
        }
    });

    function changeDateFormat(date, join_format) {
      let date_month = (date.getMonth() + 1).toString().padStart(2, '0');
      let date_day = date.getDate().toString().padStart(2, '0');
      let date_year = date.getFullYear();
      var res = '';
      res = [date_year, date_month, date_day].join(join_format);
      return String(res);
    }


    function calculatePrice(country, start, end) {
      $simuLoader.stop().fadeIn(100);
      $.ajax({
          url: "https://kw.api.gonosen.work/api/countries/price",
          method: 'post',
          crossDomain: true,
          dataType: 'json',
          data: {
            'data': [
              {
                'country_code': country,
                'from_date': start,
                'to_date': end
              },
            ]
          },
          success: function(data){
            $simuLoader.stop().fadeOut(100);
            des_price = data.data[0].total_price;
            var index = $('.active').find('a').attr('href').substring(4);
            
            var str_price = '#price_' + index;
            var $price = $(str_price);
            $price[0].innerHTML = numberWithCommas(des_price) + '円';
            
            str_price = '#des_' + index;
            $price = $(str_price);
            $price[0].innerHTML = numberWithCommas(des_price) + '円'
            
            str_price = '#des_price_' + index;
            $price = $(str_price);
            $price.val(des_price);
            
            str_price = '#daily_' + index;
            $price = $(str_price);
            var dt1 = new Date(start);
            dt1.setDate(dt1.getDate() + 1);
            var dt2 = new Date(end);
            dt2.setDate(dt2.getDate() + 1);
            $price[0].innerHTML = numberWithCommas(Math.round((dt2.getTime() - dt1.getTime()) / (1000 * 3600 * 24)) * 220) + '円';

            str_price = '#daily_price_' + index;
            $price = $(str_price);
            $price.val(numberWithCommas(Math.round((dt2.getTime() - dt1.getTime()) / (1000 * 3600 * 24)) * 220));
            
            str_price = '#total_' + index;
            $price = $(str_price);
            $price[0].innerHTML = numberWithCommas(des_price + Math.round((dt2.getTime() - dt1.getTime()) / (1000 * 3600 * 24)) * 220) + '円';

            str_price = '#total_price_' + index;
            $price = $(str_price);
            $price.val(numberWithCommas(des_price + Math.round((dt2.getTime() - dt1.getTime()) / (1000 * 3600 * 24)) * 220));
            
            var total_tabs_length = $('.tabslist__list').children().length - 1;
            
            if (total_tabs_length == 1) {
              str_price = '#des_date_' + index;
              $price = $(str_price);
              var str_dt1 = changeDateFormat(dt1, '/');
              var str_dt2 = changeDateFormat(dt2, '/');
              $price[0].innerHTML = '期間: <span>' + str_dt1 + '〜' + str_dt2.substring(5) + '</span>';
              
              str_price = '#des_area_' + index;
              $price = $(str_price);
              $price[0].innerHTML = '国名: <span>' + data.data[0].country_name_jp + '</span>';
            } else {
              for (var i = 0; i < total_tabs_length; i++) {
                var tab_index = $('.tabslist__list').children()[i].firstElementChild.getAttribute('href').substring(4);
                
                str_price = 'ul#result_list_select_' + tab_index + ' li#list_' + index + ' p#des_date_' + index;
                $price = $(str_price);
                var str_dt1 = changeDateFormat(dt1, '/');
                var str_dt2 = changeDateFormat(dt2, '/');
                $price[0].innerHTML = '期間: <span>' + str_dt1 + '〜' + str_dt2.substring(5) + '</span>';

                str_price = 'ul#result_list_select_' + tab_index + ' li#list_' + index + ' p#des_area_' + index;
                $price = $(str_price);
                $price[0].innerHTML = '国名: <span>' + data.data[0].country_name_jp + '</span>';
              }  
            }

            str_price = '#product_id_' + index;
            $price = $(str_price);
            var product_id = $price.val();

            new_product(product_id, index, data.data[0].country_name_jp, data.data[0].country_code, des_price, dt1.getTime(), dt2.getTime());
            // str_price = '#btn_apply_' + index;
            // $price = $(str_price);
            // $price.prop('href', './shopping/option?'+'num='+index+'&from='+start+'&to='+end+'&country='+data.data[0].country_name_jp+'&price='+des_price);
          }
      });
    }

    function numberWithCommas(x) {
        return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    }

    function deleteCartItem(cartitem_id, product_id, productclass_id) {
      $simuLoader.stop().fadeIn(100);
      $.ajax({
        url: '{{ url("delete_cartitem") }}',
        method: 'post',
        data: {
          'cartitem_id': cartitem_id,
          'product_id': product_id,
          'productclass_id': productclass_id
        },
        success: function(data) {
          $simuLoader.stop().fadeOut(100);
        }
      });
    }

    function new_product(product_id, index, country, country_code, price, start, end) {
      $('.applyBtn').prop('disabled', true);
      $simuLoader.stop().fadeIn(100);
      $.ajax({
        url: '{{ url("add_product") }}',
        method: 'post',
        data: {
          'product_id': product_id,
          'index': index,
          'country': country,
          'country_code': country_code,
          'from': start,
          'to': end,
          'price': price
        },
        success: function(data){
          $simuLoader.stop().fadeOut(100);
          $('.applyBtn').prop('disabled', false);
          if (data != 'fail') {
            var str_elm = '#productclass_id_' + index;
            var $elm = $(str_elm);
            $elm.val(data);
          }
        },
        error: function(err) {
          $simuLoader.stop().fadeOut(100);
          $('.applyBtn').prop('disabled', false);
        }
      });
    }
    
  </script>
